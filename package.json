{
  "name": "nestjs-pg-cron-lock",
  "version": "0.1.0",
  "description": "Distributed cron job locking for NestJS using PostgreSQL advisory locks. Ensures only one instance runs a cron job across multiple servers. No Redis required.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint": "eslint \"src/**/*.ts\" --fix"
  },
  "keywords": [
    "nestjs",
    "postgres",
    "postgresql",
    "advisory-lock",
    "cron",
    "distributed",
    "lock",
    "scheduler",
    "typeorm",
    "singleton",
    "cron-job"
  ],
  "author": "owxiang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/owxiang/nestjs-pg-cron-lock.git"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0",
    "typeorm": "^0.3.0"
  },
  "devDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/jest": "^29.5.0",
    "jest": "^29.7.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "ts-jest": "^29.1.0",
    "typeorm": "^0.3.17",
    "typescript": "^5.0.0"
  }
}
